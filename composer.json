{
    "name": "crazy/biz",
    "description": "A service layer framework.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Codeages\\Biz\\Framework\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^5.5|^7.0",
        "pimple/pimple": "~3.2",
        "doctrine/dbal": "~2.4",
        "doctrine/common": "~2.4",
        "doctrine/annotations": "~1.2",
        "davedevelopment/phpmig": "~1.2",
        "symfony/event-dispatcher": "^5.0",
        "symfony/finder": "~3.0|~4.0",
        "symfony/console": "~3.0|~4.0",
        "mtdowling/cron-expression": "~1.2",
        "webpatser/laravel-uuid": "~2.0",
        "monolog/monolog": "^1.20",
        "symfony/lock": "~3.0|~4.0",
        "symfony/polyfill-php55": "^1.4",
        "vlucas/valitron": "^1.4"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "^4.8|^5.7|^6.0|^7.0",
        "ramsey/uuid": "^3.9"
    },
    "config": {
      "bin-dir": "bin"
    },
    "suggest": {
    },
    "bin": ["bin/biz"]
}
